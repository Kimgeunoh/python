   "source": [
    "def parse_jumin(jumin):\n",
    "    year = int(jumin[:2])\n",
    "    month = int(jumin[2:4])\n",
    "    day = int(jumin[4:6])\n",
    "\n",
    "    if year >= 50:\n",
    "        full_year = 1900 + year\n",
    "    else:\n",
    "        full_year = 2000 + year\n",
    "\n",
    "    print(f\"{full_year}년 {month}월 {day}일\")\n",
    "\n",
    "\n"
   ]
  }
